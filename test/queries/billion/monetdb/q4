select s0.value,s1.value,s2.value
from (
   select f1.object as r0,f5.object as r1,f6.object as r2
   from p83 f0,p582 f1,allproperties f2,p214 f3,p527 f4,p52 f5,p51 f6
   where f0.object=157820098 and f1.subject=f0.subject and f2.object=f1.object and f3.subject=f2.subject and f3.object=82416 and f4.subject=f2.subject and f4.object=87711 and f5.subject=f2.subject and f6.subject=f2.subject
) facts,strings s0,strings s1,strings s2
where s0.id=facts.r0 and s1.id=facts.r1 and s2.id=facts.r2;
