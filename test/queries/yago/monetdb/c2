select s1.value
from strings s1, (
   select distinct t1.object as p
   from p63 t1, allproperties t2, allproperties t3, p88 t4, p88 t5, p63 t6, p63 t7
   where t2.subject=t1.subject and t3.subject=t1.subject and t4.subject=t2.object and t4.object=20368867 and t5.subject=t3.object and t5.object=20368137 and t6.subject=t2.object and t6.object=31218 and t7.subject=t3.object and t7.object=31213) as t
where s1.id=t.p;

